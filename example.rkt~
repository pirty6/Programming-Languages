#lang racket
(define get-letter (lambda (name i)
                     (cond
                       [(= 1 i) (first name)]
                       [else (get-letter (rest name)(- i 1))])))

(get-letter (cons 'j(cons 'i(cons 'z(cons 'z(cons 'z  empty))))) 5)

(define (fib number)
            (cond
              [(= number 1) 0]
              [(= number 2) 1]
              [else (+ (fib (- number 2)) (fib(- number 1)))]))

(define (fibt number a b c )
  (cond
    [(= number 1) a]
  )

(fib 4)
(fibt 4 0)